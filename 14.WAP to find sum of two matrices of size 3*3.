#include<stdio.h>
int main(){
int i,j,a[3][3],b[3][3],c[3][3];
printf("Matrix A:\n");
for(i=0;i<3;i++){
   for(j=0;j<3;j++){
	scanf("%d",&a[i][j]);
	printf("%2d",a[i][j]);}
printf("\n");
}
printf("Matrix B:\n");
for(i=0;i<3;i++){
   for(j=0;j<3;j++){
	scanf("%d",&b[i][j]);
	printf("%2d",b[i][j]);}
printf("\n");
}
printf("Sum of Matrix C:\n");
for(i=0;i<3;i++){
   for(j=0;j<3;j++){
	c[i][j]=a[i][j]+b[i][j];
	printf("%4d",c[i][j]);}
printf("\n");
}
return 0;
}
